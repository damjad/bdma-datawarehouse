<COMPONENT>
	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="0.102"
		STATUS="ALPHA"

		COMPATIBILITY="ALL"
		AUTHOR="Talend"
		RELEASE_DATE="20050320A"
		STARTABLE="false"
		SCHEMA_AUTO_PROPAGATE="true"
		DATA_AUTO_PROPAGATE="false"
		EXTENSION="org.talend.designer.scd"
		HAS_CONDITIONAL_OUTPUTS="true"
		PARTITIONING="AUTO"
	>
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>Databases/DB Specifics/PostgreSQL</FAMILY>
		<FAMILY>Business_Intelligence/DB_SCDN</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MAX_OUTPUT="0"/>
		<CONNECTOR NAME="REJECT" NOT_SHOW_IF="(DIE_ON_ERROR == 'true')" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER
			NAME="PROPERTY"
			FIELD="PROPERTY_TYPE"
			NUM_ROW="10"
			REPOSITORY_VALUE="DATABASE:POSTGRESQL"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		/>

		<PARAMETER
			NAME="USE_EXISTING_CONNECTION"
			FIELD="CHECK"
			NUM_ROW="20"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="CONNECTION"
			FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tPostgresqlConnection"
			NUM_ROW="20"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
			DYNAMIC_SETTINGS="true"
		/>

		<PARAMETER
			NAME="DB_VERSION"
			FIELD="CLOSED_LIST"
			NUM_ROW="30"
			REPOSITORY_VALUE="DB_VERSION"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<ITEMS DEFAULT="V9_X">
				<ITEM NAME="PRIOR_TO_V9" VALUE="PRIOR_TO_V9" />
				<ITEM NAME="V9_X" VALUE="V9_X" />
			</ITEMS>
		</PARAMETER>

			<PARAMETER
			NAME="HOST"
			FIELD="TEXT"
			NUM_ROW="40"
			REPOSITORY_VALUE="SERVER_NAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="PORT"
			FIELD="TEXT"
			NUM_ROW="40"
			REPOSITORY_VALUE="PORT"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>"5432"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="DBNAME"
			FIELD="TEXT"
			NUM_ROW="41"
			REPOSITORY_VALUE="SID"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		 <PARAMETER
			NAME="SCHEMA_DB"
			FIELD="TEXT"
			NUM_ROW="41"
			REPOSITORY_VALUE="SCHEMA"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TYPE"
			FIELD="TEXT"
			NUM_ROW="41"
			SHOW="false"
			REPOSITORY_VALUE="TYPE"
		>
			<DEFAULT>Postgresql</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="USER"
			FIELD="TEXT"
			NUM_ROW="50"
			REPOSITORY_VALUE="USERNAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="PASS"
			FIELD="PASSWORD"
			NUM_ROW="50"
			REPOSITORY_VALUE="PASSWORD"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TABLE"
			FIELD="DBTABLE"
			NUM_ROW="60"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SCHEMA"
			FIELD="SCHEMA_TYPE"
			NUM_ROW="69"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="70">
			 <DEFAULT/>
		</PARAMETER>

		<PARAMETER
			NAME="SURROGATE_KEY"
			FIELD="COLUMN_LIST"
			NUM_ROW="71"
			SHOW="false"
		>
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SK_CREATION"
			FIELD="CLOSED_LIST"
			NUM_ROW="71"
			SHOW="false"
		>
			<ITEMS DEFAULT="AUTO_INCREMENT">
				<ITEM NAME="AUTO_INCREMENT"	VALUE="AUTO_INCREMENT" />
				<ITEM NAME="DB_SEQUENCE"	VALUE="DB_SEQUENCE"/>
				<ITEM NAME="INPUT_FIELD"	VALUE="INPUT_FIELD"/>
				<ITEM NAME="ROUTINE"		VALUE="ROUTINE"/>
				<ITEM NAME="TABLE_MAX"		VALUE="TABLE_MAX"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="SK_DB_SEQUENCE"
			FIELD="TEXT"
			NUM_ROW="71"
			SHOW_IF="SK_CREATION == 'DB_SEQUENCE'"
			SHOW="false"
		>
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SK_INPUT_FIELD"
			FIELD="PREV_COLUMN_LIST"
			NUM_ROW="71"
			SHOW_IF="SK_CREATION == 'INPUT_FIELD'"
			SHOW="false"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SK_ROUTINE"
			FIELD="TEXT"
			NUM_ROW="71"
			SHOW_IF="SK_CREATION == 'ROUTINE'"
			SHOW="false"
		>
			<DEFAULT>Numeric.sequence("seq", 1, 1)</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SOURCE_KEYS"
			FIELD="TABLE"
			NUM_ROW="72"
			NB_LINES="2"
			SHOW="false"
		>
			<ITEMS>
			<ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="USE_SAVE_MEM_MODE"
			FIELD="CHECK"
			NUM_ROW="73"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SOURCE_KEY_INC_NULL"
			FIELD="CHECK"
			NUM_ROW="73"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L0"
			FIELD="CHECK"
			NUM_ROW="78"
			SHOW="false"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L0_FIELDS"
			FIELD="TABLE"
			NUM_ROW="79"
			NB_LINES="5"
			SHOW_IF="USE_L0 == 'true'"
			SHOW="false"
		>
			<ITEMS>
			<ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L1"
			FIELD="CHECK"
			NUM_ROW="80"
			SHOW="false"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L1_FIELDS"
			FIELD="TABLE"
			NUM_ROW="81"
			NB_LINES="5"
			SHOW_IF="USE_L1 == 'true'"
			SHOW="false"
		>
			<ITEMS>
			<ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="ONLY_UPDATE_ACTIVE"
			FIELD="CHECK"
			NUM_ROW="82"
			SHOW_IF="(USE_L1 == 'true' and USE_L2 == 'true')"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L2"
			FIELD="CHECK"
			NUM_ROW="90"
			SHOW="false"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L2_FIELDS"
			FIELD="TABLE"
			NUM_ROW="91"
			NB_LINES="5"
			SHOW_IF="USE_L2 == 'true'"
			SHOW="false"
		>
			<ITEMS>
			<ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="L2_STARTDATE_FIELD"
			FIELD="COLUMN_LIST"
			NUM_ROW="92"
			SHOW_IF="USE_L2 == 'true'"
			SHOW="false"
		>
		</PARAMETER>

		<PARAMETER
			NAME="L2_STARTDATE_VALUE"
			FIELD="CLOSED_LIST"
			NUM_ROW="92"
			SHOW_IF="USE_L2 == 'true'"
			SHOW="false"
		>
			<ITEMS DEFAULT="JOB_START_TIME">
			<ITEM NAME="JOB_START_TIME"	VALUE="JOB_START_TIME" />
			<ITEM NAME="INPUT_FIELD"	VALUE="INPUT_FIELD"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="L2_STARTDATE_INPUT_FIELD"
			FIELD="PREV_COLUMN_LIST"
			NUM_ROW="92"
			SHOW_IF="(USE_L2 == 'true' and L2_STARTDATE_VALUE == 'INPUT_FIELD')"
			SHOW="false"
		>
		</PARAMETER>

		<PARAMETER
			NAME="L2_ENDDATE_FIELD"
			FIELD="COLUMN_LIST"
			NUM_ROW="93"
			SHOW_IF="USE_L2 == 'true'"
			SHOW="false"
		>
		</PARAMETER>

		<PARAMETER
			NAME="L2_ENDDATE_VALUE"
			FIELD="CLOSED_LIST"
			NUM_ROW="93"
			SHOW_IF="USE_L2 == 'true'"
			SHOW="false"
		>
			<ITEMS DEFAULT="NULL_VALUE">
			<ITEM NAME="NULL_VALUE"   VALUE="NULL_VALUE" />
			<ITEM NAME="FIXED_VALUE"  VALUE="FIXED_VALUE"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="L2_ENDDATE_FIXED_VALUE"
			FIELD="TEXT"
			NUM_ROW="93"
			SHOW_IF="(USE_L2 == 'true' and L2_ENDDATE_VALUE == 'FIXED_VALUE')"
			SHOW="false"
		>
			<DEFAULT>"2500"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L2_ACTIVE"
			FIELD="CHECK"
			NUM_ROW="94"
			SHOW_IF="USE_L2 == 'true'"
			SHOW="false"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L2_ACTIVE_FIELD"
			FIELD="COLUMN_LIST"
			NUM_ROW="94"
			SHOW_IF="(USE_L2_ACTIVE == 'true' and USE_L2=='true')"
			SHOW="false"
		>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L2_VERSION"
			FIELD="CHECK"
			NUM_ROW="95"
			SHOW_IF="USE_L2 == 'true'"
			SHOW="false"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L2_VERSION_FIELD"
			FIELD="COLUMN_LIST"
			NUM_ROW="95"
			SHOW_IF="(USE_L2_VERSION == 'true' and USE_L2=='true')"
			SHOW="false"
		>
		</PARAMETER>

		<PARAMETER
			NAME="NOTE"
			FIELD="LABEL"
			NUM_ROW="95"
			SHOW_IF="(USE_L2_VERSION == 'true' and USE_L2=='true')"
			SHOW="false"
		>
			<DEFAULT>(Version must be an Integer)</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L3"
			FIELD="CHECK"
			NUM_ROW="100"
			SHOW="false"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L3_FIELDS"
			FIELD="TABLE"
			NUM_ROW="110"
			NB_LINES="5"
			SHOW_IF="USE_L3 == 'true'"
			SHOW="false"
		>
			<ITEMS>
			<ITEM NAME="CURRENT_VALUE" FIELD="PREV_COLUMN_LIST" />
			<ITEM NAME="PREV_VALUE" FIELD="COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="SCHEMA_REJECT"
			FIELD="SCHEMA_TYPE"
			REQUIRED="true"
			NUM_ROW="130"
			CONTEXT="REJECT"
			SHOW="true">
			<TABLE READONLY="true">
			<COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
			<COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
			</TABLE>
		</PARAMETER>

		<PARAMETER
			NAME="DIE_ON_ERROR"
			FIELD="CHECK"
			NUM_ROW="140"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
			
		<PARAMETER
	      NAME="L2_ENDDATE_TIME_DETAILS"
	      FIELD="TEXT"
	      NUM_ROW="10"
	      SHOW_IF="(USE_L2 == 'true' and L2_ENDDATE_VALUE == 'FIXED_VALUE')"
	    >
	      <DEFAULT>"12:00:00"</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER
			NAME="NOICE"
			FIELD="LABEL"
			NUM_ROW="10"
			SHOW_IF="(USE_L2 == 'true' and L2_ENDDATE_VALUE == 'FIXED_VALUE')"
		>
			<DEFAULT>(Time pattern: 'HH:mm:ss')</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="DEBUG"
			FIELD="CHECK"
			NUM_ROW="120"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Driver-Postgres9" MODULE="postgresql-42.2.5.jar" MVN="mvn:org.postgresql/postgresql/42.2.5" REQUIRED_IF="(DB_VERSION =='V9_X') AND (USE_EXISTING_CONNECTION == 'false')" />
			<IMPORT NAME="Driver-Postgres" MODULE="postgresql-8.4-703.jdbc4.jar" MVN="mvn:postgresql/postgresql/8.4-703.jdbc4" REQUIRED_IF="(DB_VERSION =='PRIOR_TO_V9') AND (USE_EXISTING_CONNECTION == 'false')" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE_UPDATED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
		<RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
		<RETURN NAME="NB_LINE_REJECTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
	</RETURNS>
</COMPONENT>
